# DevPod Start asenetcky-dev.devpod
Host asenetcky-dev.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/var/home/asenetcky/.var/app/sh.loft.devpod/data/devpod-cli" ssh --stdio --context default --user rstudio asenetcky-dev
  User rstudio
# DevPod End asenetcky-dev.devpod

Host *
  IdentityAgent ~/.1password/agent.sock
